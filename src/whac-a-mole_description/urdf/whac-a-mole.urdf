    </link>

    <joint name="base_to_linear_actuator_2" type="fixed">
        <parent link="base"/>
        <child link="linear_actuator_2"/>
        <origin xyz="15 0 0" rpy="0 0 0"/>
    </joint>

    <!-- Linear Actuator 3 -->
    <link name="linear_actuator_3">
        <visual>
            <geometry>
                <mesh filename="package://whac-a-mole_description/meshes/Linear Actuator.stl"/>
